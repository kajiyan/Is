{
  "manifest_version": 2,
  "name": "Test I\"s Extension",
  "version": "0.1",
  "description": "I\"s Extension",
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [
        "public/common/js/common.js",
        "public/js/extension/content-script/index.js"
      ]
    }
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "public/common/js/common.js",
      "public/js/extension/lib/socket.io.js",
      "public/js/extension/background/index.js"
    ]
  },
  "browser_action": {
    "default_icon": "public/images/extension/camera.png",
    "default_title": "",
    "default_popup": "popup.html"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "tabCapture",
    "activeTab",
    "http://*/*",
    "https://*/*" 
  ],
  "externally_connectable": {
    "matches": ["<all_urls>"]
  }
}
