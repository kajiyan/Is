{
  "name": "I\"s - eternal me",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "I believe in you just forever.",
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon/icon19.png"
    },
    "default_title": "I\"s - eternal me",
    "default_popup": "html/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js" : ["js/content/content-script.js"]
    }
  ],
  "web_accessible_resources": [
    "js/embeded-script.js"
  ],
  "permissions": [
    "<all_urls>"
  ]
}
